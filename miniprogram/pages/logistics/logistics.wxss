  background: #F0F0F0;
  font-size: 28rpx;
  border-radius:8rpx;
  padding: 0 10rpx;
  font-weight: normal;
}
.flex-between{
  align-items: center;
}
.flex-between .i{
  font-size: 48rpx;
  color:#CCCCCC
}
.yellowmin .msgbox {
  background: #fff;
  padding: 0 30rpx 30rpx;
  width: 690rpx;
  font-size: 28ox;
  border-radius: 12rpx;
  box-shadow: 0 0 0 3rpx #ececec;
  margin: 25rpx auto 30rpx;
}
.msgbox .h2{
  font-size: 28rpx;
  margin-bottom: 30rpx;
  padding-top: 30rpx;
}
.msgbox .flex{
  font-size: 28rpx;
  margin: 30rpx 0;
  
}
.msgbox .flex .div:nth-child(1){
  color:#CCCCCC
}
.msgbox .flex .div:nth-child(2){
  color:#333333
}
.yellowmin{
  color:#FFB606;
  border:1rpx solid #FFB606;
  border-radius: 30rpx;
  font-size: 28rpx;
  width: 160rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
}
  .msgbox{
    background: #fff;
    padding: 0 30rpx 30rpx;
    width: 85%;
    font-size: 28ox;
    border-radius: 12rpx;
    box-shadow: 0 0 0 3rpx #ececec;
    margin: 25rpx auto 30rpx;
    
  }
  .tab_title{
    color:#999999;
    font-size: 26rpx;
  }
  .tab_title .span{
    padding: 20rpx 0rpx 10rpx;
  }
  .tab_title .active{
    position: relative;
  }
  .tab_title .active:after{
    content: '';
    width: 100%;
    height: 5rpx;
    border-radius: 4rpx;
    background:#FFB606 ;
    position: absolute;
    bottom: 0;
    left: 0;
  }
  .iconbox{
    width: 125rpx;
  }
  .titlebox{
    font-size: 30rpx;
  }
  .msg_list{
    padding: 30rpx 0;
    border-bottom: 1rpx solid #cdcdcd;
    
  }
  .msg_list .time{
    position: absolute;
    right: 0;
    color:#B3B3B3;
    font-size: 24rpx;
    font-weight: normal;
    top: 35rpx;
    right: 30rpx;
  }
  .details{
    font-size: 24rpx;
    margin-top: 15rpx;
    color:#B3B3B3
  }
  .emptycenter{
    width: 600rpx;
    margin: 30rpx auto;
    display: block;
    font-size: 24rpx;
  }
  .emptybox{
    height: auto;
    width: 120rpx;
    margin-top: 20rpx;
  }