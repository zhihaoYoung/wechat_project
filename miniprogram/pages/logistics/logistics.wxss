}
.cityflex{
  position: relative;
  font-weight: bold;
  justify-content: center;
  display: flex;
  font-size: 36rpx;
  color:#333333;
  
 
}
.cityflex .b{
  font-size: 26rpx;
}
.gosendproduct{
  background: #FFB606;
  color:#fff;
  border-radius:32rpx;
  width:160rpx;
  height:64rpx;
  text-align: center;
  line-height: 64rpx;
  font-size: 28rpx;
}
.flexs{
  display: flex
}
.bg_gray{
  width: 100%;
  height: 100%;
  position: fixed;
  top:0;
  left: 0;
  background: #f7f7f7;
}
.centerbox{
  position: relative;
  z-index: 2;
}
.statusbox{
  color:#FC4E7F;
  font-size: 28rpx;
