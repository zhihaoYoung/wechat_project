/* miniprogram/pages/successpage/successpage.wxss */
page{
  background: #f7f7f7;
}
.viewbox{
  margin:0 auto;
  text-align: center;
}

.btnstyle{
  width: 330rpx;
  padding: 25rpx 0;
  border-radius: 5rpx;
  background: #fff;
}
.bgcenter{
  background: #F7F7F7;
  position: fixed;
  top:0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.centerbox .i{
  margin-top: 80rpx;
  display: block;
  font-size: 170rpx;
  color:#1fc88c
}
.centerbox .el-button{
  border-radius: 8rpx;
}
.centerbox .titlebox{
  margin-top: 40rpx;
  font-size: 36rpx;
  color:#292929
}

.centerbox .infobox{
  color:#CCCCCC;
  margin-top: 20rpx;
  font-size: 28rpx;
}
.centerbox .footerbox{
  padding: 0 30rpx;
  font-size: 32rpx;
  color:#333333
}
.centerbox .btnbox{
  width: 90%;
  background: #FFB606;
  color:#fff;
  border:1rpx solid #FFB606;
  margin: 70rpx auto 30rpx;
  display: block;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 36rpx;
}
.centerbox{
  position: relative;
  z-index: 2;
  margin-top: 50rpx;
  text-align: center;
}
.centerbox img{
  width: 140rpx;
  height: 140rpx;
  margin: 90rpx auto 0;
  display: block
}
.onlywhite{
  background: #fff!important;
  color:#333!important;
  border:none!important;
  margin: 30rpx auto 30rpx!important;
}